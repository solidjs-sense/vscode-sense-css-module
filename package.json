{
  "name": "sense-css-module",
  "author": {
    "email": "ooiss@qq.com",
    "name": "iamcco <年糕小豆汤>"
  },
  "publisher": "iamcco",
  "license": "MIT",
  "repository": "https://github.com/solidjs-sense/vscode-sense-css-module.git",
  "icon": "logo.png",
  "displayName": "sense-css-module",
  "description": "sense css module extension for VS Code",
  "version": "0.1.3",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:less"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "properties": {
        "sense-css-module.enable": {
          "type": "boolean",
          "default": false,
          "description": "enable coc-sense-css-module"
        },
        "sense-css-module.global-style-files": {
          "type": "array",
          "default": [],
          "description": "global style files"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "sense-css-module-server": "^0.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "vscode-languageclient": "^8.1.0",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  }
}
